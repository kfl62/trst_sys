<div style="width:460px;height:310px">
  <% o = find_obj(TrstHr,params[:obj_id]) %>
  <table style="width:80%; margin:auto;">
    <thead>
      <tr>
        <%= th_label_tag o, :id, nil, {:attributes => "label_attribute"} %>
        <%= th_label_tag o, :id, nil, {:attributes => "label_value"} %>
      </tr>
    </thead>
    <tbody align="left">
      <tr>
        <%= td_label_tag o, :prefix %>
        <%= td_value_tag o, :prefix %>
      </tr>
      <tr>
        <%= td_label_tag o, :lst_name %>
        <%= td_value_tag o, :lst_name %>
      </tr>
      <tr>
        <%= td_label_tag o, :frst_name %>
        <%= td_value_tag o, :frst_name %>
      </tr>
      <tr>
        <%= td_label_tag o, :mdl_name %>
        <%= td_value_tag o, :mdl_name %>
      </tr>
      <tr>
        <%= td_label_tag o, :stts %>
        <%= td_value_tag o, :stts %>
      </tr>
      <tr>
        <%= td_label_tag o, :full_name %>
        <%= td_value_tag o, :full_name %>
      </tr>
      <tr>
        <td colspan="2"><hr/></td>
      </tr>
      <%= custom_schema_definitions_html(o) %>
      <%= last_row_for  o, :show %>
    </tbody>
  </table>
</div>
