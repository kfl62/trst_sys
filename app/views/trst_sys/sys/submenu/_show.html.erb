<div style="width:460px;height:320px">
  <% o = find_obj(TrstSysSubmenu,params[:obj_id]) %>
  <table style="width:80%; margin:auto;">
    <thead>
      <tr>
        <%= th_label_tag o, :id, nil, {:attributes => "label_attribute"} %>
        <%= th_label_tag o, :id, nil, {:attributes => "label_value"} %>
      </tr>
    </thead>
    <tbody align="left">
      <tr>
        <%= td_label_tag o, :id %>
        <%= td_value_tag o, :id %>
      </tr>
      <tr>
        <%= td_label_tag o, :name %>
        <%= td_value_tag o, :name %>
      </tr>
      <tr>
        <%= td_label_tag o, :controller %>
        <%= td_value_tag o, :controller %>
      </tr>
      <tr>
        <%= td_label_tag o, :chapter %>
        <%= td_value_tag o, :chapter %>
      </tr>
      <tr>
        <%= td_label_tag o, :page %>
        <%= td_value_tag o, :page %>
      </tr>
      <tr>
        <%= td_label_tag o, :active %>
        <%= td_value_tag o, :active %>
      </tr>
      <tr>
        <%= td_label_tag o, :title %>
        <%= td_value_tag o, :title %>
      </tr>
      <tr>
        <td colspan="2"><hr/></td>
      </tr>
      <%= custom_schema_definitions_html(o) %>
      <%= last_row_for o, :show %>
    </tbody>
  </table>
</div>
